{
    "name": "add-items",
    "displayName": "Add-Items",
    "publisher": "TheFish2191",
    "description": "Create classes and other types for any Programming Language (C#, Cpp, python etc)",
    "repository": "https://github.com/thefish2191/add-items",
    "icon": "./Resources/Extension/windows.png",
    "version": "0.7.0",
    "license": "MIT",
    "engines": {
        "vscode": "^1.76.0"
    },
    "extensionDependencies": [
        "Xtendia.file-craft"
    ],
    "activationEvents": [
        "onStartupFinished"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "snippets": [
            {
                "language": "jsonc",
                "path": "/snippets/Jsonc.json"
            }
        ],
        "commands": []
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "pretest": "npm run compile && npm run lint",
        "lint": "eslint src --ext ts",
        "test": "node ./out/test/runTest.js"
    },
    "devDependencies": {
        "@types/glob": "^8.1.0",
        "@types/mocha": "^10.0.1",
        "@types/node": "16.x",
        "@types/vscode": "^1.76.0",
        "@typescript-eslint/eslint-plugin": "^5.56.0",
        "@typescript-eslint/parser": "^5.56.0",
        "@vscode/test-electron": "^2.3.0",
        "eslint": "^8.36.0",
        "glob": "^8.1.0",
        "mocha": "^10.2.0",
        "typescript": "^4.9.5"
    },
    "dependencies": {
        "comment-json": "^4.2.3"
    }
}
